    <h2 class="h2OBJ">Experiment</h2>

    <form action="/profile" method="POST"> 
        <div class="TD TDfirst">
        <table>
        <div class="form-group">
            <tr>
                <th>No. Of Experiment</th>
                <th>Year Of Execution</th>
                <th>Duration</th>
                <th>Treatment Details</th>
                <th>Data Details</th>
            </tr>
            <% for (let i = 0; i < Math.max(ExecutionT.length, DurationT.length, TreatmentDT.length, DataDT.length); i++) { %>
                <tr>
                    <td data-heading="Number"><%= i+1 %></td>
                    <td data-heading="Year Of Execution"><%= i < ExecutionT.length ? ExecutionT[i] : '' %></td>
                    <td data-heading="Duration"><%= i < DurationT.length ? DurationT[i] : '' %></td>
                    <td data-heading="Treatment Details"><%= i < TreatmentDT.length ? TreatmentDT[i] : '' %></td>
                    <td data-heading="Data Details"><%= i < DataDT.length ? DataDT[i] : '' %></td>
                    
                </tr>
                
                <% } %>
                
                
                
                <tr>
                    <td><input type="text"></td>
                    <td><input type="text" placeholder="Year Of Execution" name="ExecutionT"></td>
                    <td><input type="text" placeholder="Duration" name="DurationT"></td>
                    <td><input type="text" placeholder="Treatment Details" name="TreatmentDT"></td>
                    <td><input type="text" placeholder="Data Details" name="DataDT"></td>
                    <td><button id="job-post-button" class="btn btn-success mt-3" type="submit">Submit</button></td>
                </tr>
    </div>
    </table>
</div>
        </form>










